        },
        {
            "name": "Community contributions",
            "homepage": "http://github.com/Sylius/Sylius/contributors"
        }
    ],
    "require": {
        "php": "^7.0",

        "sylius/registry": "^1.0",
        "sylius/resource": "^1.0"
    },
    "require-dev": {
        "phpspec/phpspec": "^4.0",
        "phpunit/phpunit": "^6.3",
        "doctrine/orm": "^2.5"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": { "Fosyl\\ReportOne\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\Fosyl\\ReportOne\\": "spec/",
            "test\\Fosyl\\ReportOne\\": "test/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
